<% provide(:title, 'Alle User') %>

<header class="jumbotron_subhead" id="overview">
	<div class="container">
		<h1>Alle User</h1>
	</div>
</header>

<div class="container textarea">
	<div class="row">
	  <div class="span9 offset1">
			<%= will_paginate %>
				<ul class="users">
				  <% @users.each do |user| %>
				    <%= render user %>
				  <% end %>
				</ul>
			<%= will_paginate %>	
	  </div>
	</div>
</div>